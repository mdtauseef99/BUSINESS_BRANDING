@model BizzBranding.Areas.Admin.Models.MembershipWebModel
@{
    ViewBag.Title = "ViewMembership";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

@*<h2>@Html.DisplayFor(x => x.MembershipTitle)</h2>*@
@Html.HiddenFor(x => x.MembershipID)
<div style="box-shadow: 0 0 5px; border-radius: 10px;">
    <div class="emptyspace"></div>

  
    <div class="row">
        <div ><span class="rowleft">Access Plan Name</span></div>
        <div class="rowright">
            @Html.DisplayFor(x => x.AccessPlanName)
        </div>
    </div>
    <div class="row">
        <div><span class="rowleft">Business Name</span></div>
        <div class="rowright">
            @Html.DisplayFor(x => x.BusinessName)
        </div>
    </div>

    <div class="row">
        <div ><span class="rowleft">Activated On</span></div>
        <div class="rowright">
            @Html.DisplayFor(x => x.ActivatedOn)
        </div>
    </div>
    <div class="row">
        <div ><span class="rowleft">Expires On</span></div>
        <div class="rowright">
            @Html.DisplayFor(x => x.ExpiresOn)
        </div>
    </div>

    <div class="row">
        <div class="rowleft">@Html.LabelFor(x => x.IsActive)</div>
        <div class="rowright">
            @Html.DisplayTextFor(x => x.IsActive)
        </div>
    </div>



    <div class="row">
        <div class="rowleft">
        </div>
        <div class="rowright">
            <a href="@Url.Action("Membership","MasterData")" class="form-reset">Back</a>

        </div>

    </div>
</div>
