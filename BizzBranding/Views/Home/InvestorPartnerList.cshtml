                                    <div class="modal-footer">
                                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>

                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-------------------------------------------------------popup-end--------------------------------->
                        <div class="col-sm-3 brand-list-right">
                            @*<h2 style="font-size: 17px;">What is Investors Partnering?</h2>*@
                            <p  >@Html.Raw(Model.Description)</p>
                          
                        </div>
                    </div>
                </div>
            </div>
        </section>



